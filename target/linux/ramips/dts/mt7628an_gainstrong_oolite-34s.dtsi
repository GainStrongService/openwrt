#include "mt7628an.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	aliases {
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-upgrade = &led_system;
	};

	leds {
		compatible = "gpio-leds";

		led_system: led_gpio_37 {
			label = "led_gpio_37";
			gpios = <&gpio 37 GPIO_ACTIVE_HIGH>;
		};

		led_gpio_38 {
			label = "led_gpio_38";
			gpios = <&gpio 38 GPIO_ACTIVE_HIGH>;
		};

		led_gpio_39 {
			label = "led_gpio_39";
			gpios = <&gpio 39 GPIO_ACTIVE_HIGH>;
		};

		led_gpio_40 {
			label = "led_gpio_40";
			gpios = <&gpio 40 GPIO_ACTIVE_HIGH>;
		};

		led_gpio_41 {
			label = "led_gpio_41";
			gpios = <&gpio 41 GPIO_ACTIVE_HIGH>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	sdhci@10130000 {
		status = "okay";
	};

	spi-gpio {
		compatible = "spi-gpio";
		#address-cells = <1>;
		cs-gpios = <&gpio 2 0>;
		gpio-sck = <&gpio 0 0>;
		gpio-mosi = <&gpio 3 0>;
		gpio-miso = <&gpio 1 0>;
		num-chipselects = <1>;
		spidev@0 {
			#address-cells = <1>;
			compatible = "linux,spidev";
			spi-max-frequency = <10000000>;
			reg = <0>;
		};
	};
};

&state_default {
	gpio {
		groups = "gpio", "i2c", "i2s", "uart1", "uart2", "pwm0", "pwm1", "refclk", "p0led_an", "p1led_an", "p2led_an", "p3led_an", "p4led_an", "wled_an";
		function = "gpio";
	};
};

&spi0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&spi_pins>, <&spi_cs1_pins>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			firmware: partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
			};
		};
	};

	spidev@1 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "linux,spidev";
		reg = <1>;
		spi-max-frequency = <10000000>;
	};
};

&ethernet {
	mtd-mac-address = <&factory 0x28>;
};

&wmac {
	status = "okay";
};

&pcie {
	status = "okay";
};
