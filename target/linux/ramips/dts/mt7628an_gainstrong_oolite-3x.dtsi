#include "mt7628an.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	aliases {
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-upgrade = &led_system;
	};

	leds {
		compatible = "gpio-leds";

		led_gpio_3 {
			label = "led_gpio_3_Ethernet";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
		};

		led_gpio_46 {
			label = "led_gpio_46_WiFi_5G";
			gpios = <&gpio 46 GPIO_ACTIVE_LOW>;
		};

		led_gpio_44 {
			label = "led_gpio_44_WiFi_2G";
			gpios = <&gpio 44 GPIO_ACTIVE_LOW>;
		};

		led_gpio_0 {
			label = "led_gpio_0_USB";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			trigger-sources = <&ohci_port1>, <&ehci_port1>;
			linux,default-trigger = "usbport";
		};

		led_gpio_37 {
			label = "led_gpio_37_SDCard";
			gpios = <&gpio 37 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		led_system: led_gpio_39 {
			label = "led_gpio_39_PowerGreen";
			gpios = <&gpio 39 GPIO_ACTIVE_HIGH>;
		};

		led_gpio_2 {
			label = "led_gpio_2_PowerRed";
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
		};
	};

	gpio_export {
		compatible = "gpio-export";
		#size-cells = <0>;

		usbpower {
			gpio-export,name = "usbpower";
			gpio-export,output = <1>;
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
		};

		power_control {
			gpio-export,name = "power_control";
			gpio-export,output = <1>;
			gpios = <&gpio 43 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		power {
			label = "power";
			gpios = <&gpio 40 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_POWER>;
		};

		wps {
			label = "wps";
			gpios = <&gpio 41 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		soc {
			label = "soc";
			gpios = <&gpio 42 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_0>;
			linux,input-type = <EV_SW>;
		};
	};

	sdhci@10130000 {
		status = "okay";
	};


	bat: battery {
		compatible = "simple-battery";
		voltage-min-design-microvolt = <3200000>;
		energy-full-design-microwatt-hours = <12580000>;
		charge-full-design-microamp-hours = <3400000>;
		precharge-current-microamp = <256000>;
		charge-term-current-microamp = <128000>;
		constant-charge-current-max-microamp = <1200000>;
		constant-charge-voltage-max-microvolt = <4200000>;
	};

};

&state_default {
	gpio {
		groups = "gpio", "i2s", "uart1", "refclk", "p0led_an", "p1led_an", "p2led_an", "p3led_an", "p4led_an", "wled_an";
		function = "gpio";
	};
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				//read-only;  //recommend to enable this option for release firmware
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				//read-only;  //recommend to enable this option for release firmware
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				//read-only;  //recommend to enable this option for release firmware
			};

			firmware: partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
			};

			all: partition@all {
				label = "all";
				//read-only;  //recommend to enable this option for release firmware
			};
		};
	};
};

&ethernet {
	mtd-mac-address = <&factory 0x4>;
    mtd-mac-address-increment = <1>;
};

&wmac {
	status = "okay";
};

&pcie {
	status = "okay";
};

&pcie0 {
	wifi@0,0 {
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x8000>;
		ieee80211-freq-limit = <5000000 6000000>;
	};
};


&i2c {
	status = "okay";
	clock-frequency = <100000>;
    
	bq27546@55 {
		compatible = "ti,bq27546";
		reg = <0x55>;
		monitored-battery = <&bat>;
    };

	/* charger */
	mp2731@4b {
		compatible = "mps,mp2624";
		reg = <0x4b>;
		monitored-battery = <&bat>;
		/* interrupts are broken */
		regulators {
			vsys_reg: vsys_regulator {
				regulator-compatible = "bq2429x-vsys";
				regulator-name = "vsys";
				regulator-min-microvolt = <3500000>;
				regulator-max-microvolt = <4200000>;
				regulator-always-on;
				regulator-boot-on;
			};
			otg_reg: otg_regulator {
				regulator-compatible = "bq2429x-otg";
				regulator-name = "otg";
				regulator-min-microvolt = <4900000>;
				regulator-max-microvolt = <5100000>;
			};
		};
	};

    atsha204a@64 {
        compatible = "atmel,atsha204a";
        reg = <0x64>;
    };
};

&sdhci {
	status = "okay";	
};
