#include "mt7628an.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "gainstrong,oolite-v32", "mediatek,mt7628an-soc";
	model = "GainStrong Oolite V3.2";

	aliases {
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-upgrade = &led_system;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_system: system {
			label = "green:system";
			gpios = <&gpio 44 GPIO_ACTIVE_HIGH>;
		};
	};

	spi-gpio1 {
		compatible = "spi-gpio";
		#address-cells = <1>;
		gpio-sck = <&gpio 0 0>;
		cs-gpios = <&gpio 2 0>;
		gpio-mosi = <&gpio 3 0>;
		gpio-miso = <&gpio 1 0>;
		num-chipselects = <1>;
		spidev@0 {
			#address-cells = <1>;
			compatible = "linux,spidev";
			spi-max-frequency = <1000000>;
			reg = <0>;
		};
	};

	spi-gpio2 {
		compatible = "spi-gpio";
		#address-cells = <1>;
		gpio-sck = <&gpio 14 0>;
		cs-gpios = <&gpio 16 0>;
		gpio-mosi = <&gpio 17 0>;
		gpio-miso = <&gpio 15 0>;
		num-chipselects = <1>;
		spidev@0 {
			#address-cells = <1>;
			compatible = "linux,spidev";
			spi-max-frequency = <1000000>;
			reg = <0>;
		};
	};

	spi-gpio3 {
		compatible = "spi-gpio";
		#address-cells = <1>;
		gpio-sck = <&gpio 18 0>;
		cs-gpios = <&gpio 20 0>;
		gpio-mosi = <&gpio 21 0>;
		gpio-miso = <&gpio 19 0>;
		num-chipselects = <1>;
		spidev@0 {
			#address-cells = <1>;
			compatible = "linux,spidev";
			spi-max-frequency = <1000000>;
			reg = <0>;
		};
	};

	// spi-gpio4 {
	// 	compatible = "spi-gpio";
	// 	#address-cells = <1>;
	// 	gpio-sck = <&gpio 38 0>;
	// 	cs-gpios = <&gpio 40 0>;
	// 	gpio-mosi = <&gpio 41 0>;
	// 	gpio-miso = <&gpio 39 0>;
	// 	num-chipselects = <1>;
	// 	spidev@0 {
	// 		#address-cells = <1>;
	// 		compatible = "linux,spidev";
	// 		spi-max-frequency = <1000000>;
	// 		reg = <0>;
	// 	};
	// };
};

&state_default {
	gpio {
		groups = "gpio", "i2c", "i2s", "spi cs1", "uart1", "uart2", "pwm0", "pwm1", "refclk", "p0led_an", "p1led_an", "p2led_an", "p3led_an", "p4led_an", "wled_an";
		function = "gpio";
	};
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				compatible = "denx,uimage";
				reg = <0x50000 0xfb0000>;
				label = "firmware";
			};

			all {
				reg = <0x0 0x1000000>;
				label = "all";
				read-only;
			};
		};
	};
};

&ethernet {
	status = "okay";

	nvmem-cells = <&macaddr_factory_4>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <1>;
};

&factory {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_factory_4: macaddr@4 {
		reg = <0x4 0x6>;
	};
};

&wmac {
	status = "okay";
	mediatek,mtd-eeprom = <&factory 0x0>;
};

&pcie {
	status = "okay";
};

// 注意：GPIO14-29（含SD接口）不能与LAN1-LAN4同时启用
// Note: GPIO14-29 (including SD interface) cannot be enabled simultaneously with LAN1-LAN4
&sdhci {
	status = "okay";
};