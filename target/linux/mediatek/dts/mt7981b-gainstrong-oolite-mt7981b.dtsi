#include "mt7981.dtsi"

/ {
	model = "GainStrong Oolite-MT7981B";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	aliases {
		serial0 = &uart0;
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-running = &led_system;
		led-upgrade = &led_system;
	};

	leds {
		compatible = "gpio-leds";

		led_system: led_blue_system {
			label = "led_blue_system";
			gpios = <&pio 35 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		led_blue_wifi {
			label = "led_blue_wifi";
			gpios = <&pio 34 GPIO_ACTIVE_HIGH>;
		};
	};
};

&uart0 {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&eth {
	pinctrl-names = "default";
	pinctrl-0 = <&mdio_pins>;

	status = "okay";

    // RTL8221B-VB-CG 2.5G Ethernet port
	// On the development board
	eth0: mac@0 {
		compatible = "mediatek,eth-mac";
		reg = <0>;
		phy-mode = "2500base-x";
		phy-handle = <&phy5>;
	};

	// CPU built-in Gigabit Ethernet
	eth1: mac@1 {
		compatible = "mediatek,eth-mac";
		reg = <1>;
		phy-mode = "gmii";
		phy-handle = <&int_gbe_phy>;
	};
};

&mdio_bus {
	phy5: ethernet-phy@5 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <5>;
		reset-assert-us = <100000>;
		reset-deassert-us = <100000>;
		reset-gpios = <&pio 39 GPIO_ACTIVE_LOW>;
		realtek,aldps-enable;
		realtek,led-link-select = <0xa7 0x0 0x0>;
	};
};

&mmc0 {
	pinctrl-names = "default", "state_uhs";
	pinctrl-0 = <&mmc0_pins_default>;
	pinctrl-1 = <&mmc0_pins_uhs>;
	bus-width = <8>;
	max-frequency = <52000000>;
	cap-mmc-highspeed;
	vmmc-supply = <&reg_3p3v>;
	non-removable;
	status = "okay";
};

&pio {
	mmc0_pins_default: mmc0-pins-default {
		mux {
			groups = "emmc_45";
			function = "flash";
		};
	};

	mmc0_pins_uhs: mmc0-pins-uhs {
		mux {
			groups = "emmc_45";
			function = "flash";
		};
	};
};

&usb_phy {
	status = "okay";
};

&xhci {
	mediatek,u3p-dis-msk = <0x0>;
	phys = <&u2port0 PHY_TYPE_USB2>,
	       <&u3port0 PHY_TYPE_USB3>;
	status = "okay";
};

&wifi {
	status = "okay";
};